{
  "name": "@lifeart/ember-language-server",
  "description": "A language server implementation for Ember.js projects.",
  "version": "2.4.2",
  "contributors": [
    "Aleksandr Kanunnikov <lifeart92@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "ember",
    "language-server",
    "ember-language-server"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeart/ember-language-server.git"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": "bin/ember-language-server.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "@glimmer/syntax": "^0.73.1",
    "@lifeart/ember-extract-inline-templates": "2.1.0",
    "ast-types": "^0.14.2",
    "dag-map": "^2.0.2",
    "ember-meta-explorer": "1.1.0",
    "ember-template-recast": "^5.0.3",
    "find-up": "^5.0.0",
    "flat": "^5.0.2",
    "fuzzaldrin": "^2.1.0",
    "json-parse-ast": "^1.0.8",
    "lodash": "^4.17.20",
    "memoizee": "^0.4.15",
    "picomatch": "^2.3.0",
    "semver": "^7.3.5",
    "vscode-jsonrpc": "6.0.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-protocol": "^3.16.0",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-uri": "^3.0.2",
    "yaml": "^2.0.0-8"
  },
  "devDependencies": {
    "@babel/types": "^7.12.12",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/estree": "^0.0.46",
    "@types/flat": "^5.0.2",
    "@types/fuzzaldrin": "^2.1.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.167",
    "@types/memoizee": "^0.4.5",
    "@types/node": "^12.12.5",
    "@types/picomatch": "^2.3.0",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "broccoli-test-helper": "^2.0.0",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lerna-changelog": "^0.8.2",
    "lint-staged": "^10.5.4",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nyc": "^15.1.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.3",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": true,
    "printWidth": 160,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "parser": "typescript"
  },
  "scripts": {
    "changelog": "lerna-changelog",
    "clean": "rimraf lib/",
    "build:bundle:node": "webpack --mode production",
    "build:bundle:worker": "webpack --mode production && node ./fix-worker-bundle.js",
    "compile": "tsc --skipLibCheck -p .",
    "lint": "eslint ./{src,test}/**/*.ts",
    "prepublish": "yarn clean && yarn compile",
    "test": "jest",
    "test:coverage": "rimraf inst/ && nyc instrument lib inst && nyc --no-clean --reporter=clover --reporter=json --reporter=lcov yarn run jest --no-coverage",
    "watch": "tsc --watch -p .",
    "prepare": "husky install"
  },
  "volta": {
    "node": "14.18.1",
    "yarn": "1.22.4"
  }
}
